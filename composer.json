{
    "name": "andrey-helldar/pretty-routes",
    "description": "Pretty routes for Laravel 5+",
    "keywords": ["laravel", "routes", "pretty", "pretty-routes"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        },
        {
            "name": "Gary Green",
            "email": "holegary@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/pretty-routes/issues",
        "source": "https://github.com/andrey-helldar/pretty-routes"
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "doctrine/annotations": "^1.0",
        "illuminate/contracts": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/view": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9|^1.0",
        "orchestra/testbench": "^3.4|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^5.7|^6.0|^7.0|8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PrettyRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "PrettyRoutes\\BladeServiceProvider",
                "PrettyRoutes\\ServiceProvider"
            ]
        }
    }
}
